    "preview": "vite preview"
  },
  "devDependencies": {
    "electron": "^34.0.1",
    "electron-builder": "^24.13.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "panzoom": "^9.4.3"
  },
  "main": "dist-electron/main.js"
}
